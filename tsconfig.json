{
    "compilerOptions": {
        "outDir": ".",
        "target": "es2015",
        "module": "commonjs",
        "strict": true,
        "noImplicitAny": true,
        "alwaysStrict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
    ]
}